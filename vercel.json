{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "env": {
    "NPM_CONFIG_PRODUCTION": "false"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "^/static/(.*)",
      "dest": "/client/static/$1"
    },
    {
      "src": "/(.*)\\.png",
      "dest": "/client/$1.png"
    },
    {
      "src": "/(.*)",
      "dest": "/client/index.html"
    }
  ]
}
